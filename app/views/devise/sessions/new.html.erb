<div class="form">
  <div class="form-top">
    <%= link_to get_started_path do %> < Cancel <% end %>
    <h2 class="form-title">Log In</h2>
  </div>
  <div class="form-view not-logged">
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div>
        <%= f.email_field :email, placeholder: :email, autofocus: true, autocomplete: "email" %>
      </div>
      <div>
        <%= f.password_field :password, placeholder: :password, autocomplete: "current-password" %>
      </div>
      <div class="checks">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
      </div>
      <div class="actions">
        <%= f.submit "Log in" %>
      </div>
      <div class="other-actions">
        <%= render "devise/shared/links" %>
      </div>
    <% end %>
  </div>
</div>
